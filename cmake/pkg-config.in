Version: @LIBSETTINGS_VERSION_MAJOR@.@LIBSETTINGS_VERSION_MINOR@.@LIBSETTINGS_VERSION_PATCH@
Libs: -L${libdir} -lsettings
Cflags: -I${includedir}
